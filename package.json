{
  "name": "vue-this-store",
  "displayName": "VueThis$Store",
  "version": "0.0.7",
  "publisher": "IWANABETHATGUY",
  "description": "一个通过ast和正则表达式，获取store中的所有定义信息，并在.vue文件中给出vuex中state，actions, getters, mutations提示的vscode插件",
  "icon": "favicon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/IWANABETHATGUY/vue-this-store"
  },
  "bugs": {
    "url": "https://github.com/IWANABETHATGUY/vue-this-store/issues"
  },
  "engines": {
    "vscode": "^1.29.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:vue"
  ],
  "contributes": {
    "commands": [
      {
        "command": "VueThis$Store.specify entrance path",
        "title": "自定义入口路径specify entrance path"
      }
    ]
  },
  "main": "./out/extension",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/chokidar": "^1.7.5",
    "@types/node": "^10.12.11",
    "tslint": "^5.11.0",
    "typescript": "^3.2.1",
    "vscode": "^1.1.22"
  },
  "dependencies": {
    "@babel/parser": "^7.1.6",
    "@babel/traverse": "^7.1.6",
    "@babel/types": "^7.1.6",
    "chokidar": "^2.0.4"
  }
}
